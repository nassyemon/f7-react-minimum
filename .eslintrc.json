{
    "extends": [
        "@cordova/eslint-config/node",
        "react-app",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "react/prop-types": "off",
        "import/prefer-default-export": "off",
        "import/no-cycle": "off",
        "no-multi-assign": "off",
        "no-console": "off",
        "react/jsx-filename-extension": "off",
        "indent": "off",
        "react/jsx-indent": "off",
        "prettier/prettier": "off"
    },
    "globals": {
        "window": true,
        "Camera": true,
        "StatusBar": true,
        "cordova": true
    },
    "settings": {},
    "plugins": [
        "import",
        "jest-dom"
    ]
}